# URL da API do Redmine
REDMINE_URL=https://redmine.platform.brobot.com.br

# Banco de dados PostgreSQL
DATABASE_URL=postgresql://usuario:senha@localhost:5432/redmine_dashboard_development

# Redis para cache e background jobs
